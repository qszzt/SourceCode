package test4;

public class Person {
	String name;
    int age;
    public Person(String n,int a) {
  	  name=n;
  	  age=a;
    }
    public String getName() {
      	return name;
      }
    public void setAge(int a) {
      	age=a;
      }
    public static void main(String[] args){
  		Person abc=new Person("ÇñË¬",18);
  		System.out.println(abc.getName());
  		abc.setAge(17);
    }
}
