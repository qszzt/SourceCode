package test4;

public class Person {
	String name;
    int age;
    public Person(String n,int a) {
  	  name=n;
  	  age=a;
    }
    public String getName() {
      	return name;
      }
    public int setAge(int a) {
      	age=a;
      	return age;
      }
    public String toString() {
    	return name;
    }
    public static void main(String[] args){
  		Person abc=new Person("ÇñË¬",18);
  		System.out.println(abc);
  	
  		
    }
}
